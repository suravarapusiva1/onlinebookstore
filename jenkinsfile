    agent any
    stages{
        stage("built stage"){
            steps{
                sh "mvn clean install"
            }
        }
       stage("test stage"){
            steps{
                sh "mvn test"
            }
        }
        stage("sonarcube anyalsis"){
            environment{
                def scannerHome = tool 'sonarQubescanner'
            }
            steps {
                withSonarQubeEnv('sonarserver 9.5') {
                sh "mvn clean verify sonar:sonar \
                    -Dsonar.projectKey=online \
                    -Dsonar.host.url=http://18.212.219.78:8081 \
                    -Dsonar.login=sqp_e57a823bfe724b25e14f4d3e39ccff24885527a7"
                }
            }
        }
    }
}