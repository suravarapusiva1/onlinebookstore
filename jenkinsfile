pipeline{
    agent any 
    stages{
        stage("built stage"){
            steps{
                sh "mvn clean install"
            }
        }
       stage("test stage"){
            steps{
                sh "mvn test"
            }
        }
        stage("sonarcube anyalsis"){
            environment{
                def scannerHome = tool 'sonarQubescanner'
            }
            steps {
                withSonarQubeEnv('sonarserver') {

                    sh "${scannerHome} mvn clean package sonar:sonar"
                }
            }
        }
    }
}