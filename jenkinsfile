pipeline{
    agent any 
    stages{
        stage("built stage"){
            steps{
                sh "mvn clean install"
            }
        }
       stage("test stage"){
            steps{
                sh "mvn test"
            }
        }
        stage("sonarcube anyalsis"){
            steps {
                withSonarQubeEnv('sonarserver') {
                 sh "mvn sonar:sonar"
                }
            }
        }
    }
}