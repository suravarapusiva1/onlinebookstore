pipeline{
    agent any 
    stages{
        stage("built stage"){
            steps{
                sh "mvn clean install"
            }
        }
       stage("test stage"){
            steps{
                sh "mvn test"
            }
        }
        stage("sonarcube anyalsis"){
            environment{
                def scannerHome = tool('sonarQubescanner 9.5')
            }
            steps {
                withSonarQubeEnv('sonarserver 9.5') {
                 sh "mvn sonar:sonar"
                }
            }
        }
    }
}