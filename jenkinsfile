pipeline{
    agent any 
    stages{
        stage("built stage"){
            steps{
                sh "mvn clean install"
            }
        }
       stage("test stage"){
            steps{
                sh "mvn test"
            }
        }
        stage("sonarcube anyalsis"){
            steps{
                def mvn = tool 'sonarQubescanner';
                withSonarQubeEnv('sonarserver 9.5') {
                    sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=onlinebookstore"
                 }
            }
        }       
    }
}
